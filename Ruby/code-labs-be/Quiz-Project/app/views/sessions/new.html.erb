<%= form_with(model: @user) do |form|%>
    <div>
        <%= form.label :username %>
        <%= form.text_field :username %>
    </div>
    <div>
        <%= form.label :password %>
        <%= form.text_field :password %>
    </div>
    <div>
        <%= form.submit "Log In" %>
    </div>
<%end%>