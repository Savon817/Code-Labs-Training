<%= render partial: 'errors', locals: {blog: @blog} %>

<%= form_with(model: blog) do |form|%>
    <div>
        <%= form.label "Title", style: "display: block"%>
        <%= form.text_field :title %>
    </div>
    <div>
        <%= form.label "Description", style: "display: block"%>
        <%= form.text_area :description %>
    </div>
    <div>
        <%= form.submit "Submit" %>
    </div>
<% end %>
